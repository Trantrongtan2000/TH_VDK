#include <16F877A.h>
#fuses NOWDT, HS, NOPUT, NOPROTECT, NODEBUG, NOBROWNOUT, NOLVP
#use delay(clock=8M)
#INT_TIMER1
void qled()
{
   output_d(0xff);
   output_b(0x91);
   output_d(0xff);
   output_b(0x61);
   output_d(0xff);
   output_b(0x31);
}
void main(void)
{ 
qled();
enable_interrupts(GLOBAL);
enable_interrupts(INT_TIMER1);
setup_timer_0(RTCC_DIV_64|RTCC_INTERNAL);
while(1);
}
